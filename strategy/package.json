{
  "name": "@palabola86/trade-strategy-builder",
  "version": "1.1.14",
  "description": "A visual trading strategy builder component for React/Next.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "STRATEGY_BUILDER_QUICK_REFERENCE.md",
    "STRATEGY_BUILDER_SETUP.md",
    "STRATEGY_BUILDER_SUMMARY.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "next": "16.0.7"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-collapsible": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "latest",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-select": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "tailwind-merge": "^2.5.5"
  },
  "keywords": [
    "react",
    "nextjs",
    "crypto",
    "trading",
    "strategy",
    "builder",
    "dnd",
    "drag-and-drop"
  ],
  "author": "Palabola86",
  "license": "MIT",
  "homepage": "https://palabola.github.io/TradeStrategyBuilder",
  "repository": {
    "type": "git",
    "url": "https://github.com/Palabola/TradeStrategyBuilder.git"
  }
}
